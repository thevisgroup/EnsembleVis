{
  "name": "ensemble-vis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "eslint \"src/*\" --fix && prettier \"src/*.{js,vue}\" --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/vue-fontawesome": "2.0.0",
    "bootstrap-vue": "2.18.1",
    "core-js": "3.6.5",
    "csv": "^5.3.2",
    "d3": "6.2.0",
    "d3-sankey": "^0.12.3",
    "darkmode-js": "1.5.7",
    "jquery": "3.5.1",
    "lucene": "2.1.1",
    "md5": "2.3.0",
    "moment": "2.29.1",
    "plotly.js": "1.57.1",
    "vue": "2.6.12",
    "vue-router": "3.4.8",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-eslint": "4.5.7",
    "@vue/cli-service": "4.5.8",
    "babel-eslint": "10.1.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.1.0",
    "node-dir": "0.1.17",
    "prettier": "2.1.2",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
